%% -*- erlang -*-
[
 {sasl, [
         {sasl_error_logger, false}
        ]},

 {lager, [
          {error_logger_redirect, true},
          {suppress_supervisor_start_stop, true},
          {log_root, "log"},
          {crash_log, "crash.log"},
          {colored, true},
          {handlers, [
                      {lager_file_backend, [{file, "console.log"}, {level, info}]},
                      {lager_file_backend, [{file, "error.log"}, {level, error}]}
                     ]}
         ]},

 {ubx, [
        {filename, "spidev1.0"},
        {gpio, 68}
       ]},

 {dogstatsd, [
              {vm_stats, false},
              {global_prefix, "config"}
             ]}
].
